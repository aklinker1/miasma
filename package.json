    "@nuxtjs/tailwindcss": "^6.7.2",
    "@types/docker-modem": "^3.0.2",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.merge": "^4.6.7",
    "@unocss/nuxt": "^0.53.1",
    "@unocss/preset-icons": "^0.53.1",
    "@vueuse/core": "^10.2.0",
    "@vueuse/nuxt": "^10.2.0",
    "code-block-writer": "^12.0.0",
    "daisyui": "^3.1.1",
    "env-cmd": "^10.1.0",
    "husky": "^8.0.3",
    "nuxt": "3.5.3",
    "openapi-types": "^12.1.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "tsx": "^3.12.7",
    "vitepress": "1.0.0-beta.2",
    "yaml": "^2.3.1"
  },
  "dependencies": {
    "@types/node": "18",
    "autoprefixer": "^10.4.14",
    "deep-object-diff": "^1.1.9",
    "docker-modem": "^3.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "postcss": "^8.4.24",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vue": "^3.3.4",
    "vue-query": "^1.26.0",
    "vue-tsc": "^1.8.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack",
        "react"
      ]
    }
