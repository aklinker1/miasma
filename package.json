{
  "version": "2.0.2",
  "private": true,
  "type": "module",
  "engines": {
    "pnpm": "8"
  },
  "scripts": {
    "compile": "nuxi typecheck",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "gen": "tsx scripts/generate-docker-types.ts",
    "postinstall": "nuxt prepare",
    "prepare": "simple-git-hooks",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docker:build": "docker build . -t aklinker1/miasma:local",
    "docker:run": "env-cmd -- docker run -it --init -p 3000:3000 -v \"${MIASMA_DOCKER_SOCKET:-/var/run/docker.sock}:/var/run/docker.sock\" aklinker1/miasma:local"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.106",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@types/docker-modem": "^3.0.3",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "18",
    "@unocss/nuxt": "^0.55.3",
    "@unocss/preset-icons": "0.53.1",
    "@vueuse/core": "^10.4.0",
    "@vueuse/nuxt": "^10.4.0",
    "autoprefixer": "^10.4.15",
    "code-block-writer": "^12.0.0",
    "daisyui": "^3.6.3",
    "env-cmd": "^10.1.0",
    "lint-staged": "^14.0.1",
    "openapi-types": "^12.1.3",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "simple-git-hooks": "^2.9.0",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitepress": "1.0.0-rc.4",
    "vue-tsc": "^1.8.8",
    "yaml": "^2.3.1"
  },
  "dependencies": {
    "deep-object-diff": "^1.1.9",
    "docker-modem": "^3.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "nuxt": "3.5.3",
    "vue": "^3.3.4",
    "vue-query": "^1.26.0"
  },
  "pnpm": {
    "overrides": {
      "node-fetch-native": "1.2.0"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack",
        "react"
      ]
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "prettier --write"
  }
}
